<MudDialog>
    <DialogContent>
        <MudText>@Context.Url</MudText>
        <MudText>@Context.Order</MudText>
        <MudText>@Context.Name</MudText>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">Отменить</MudButton>
        <MudButton Color="Color.Secondary" Variant="Variant.Filled" OnClick="Delete">Удалить</MudButton>
    </DialogActions>
</MudDialog>

@code {

    [CascadingParameter] MudDialogInstance MudDialog { get; set; }

    [Parameter] public SitePage Context { get; set; }

    void Delete() => MudDialog.Close(DialogResult.Ok(Context));
    void Cancel() => MudDialog.Cancel();
}
